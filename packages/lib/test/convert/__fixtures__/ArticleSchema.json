    "FeatureAndBenefit": {
      "title": "Feature and Benefit",
      "type": "object",
      "properties": {
        "featureAndBenefitCode": {
          "type": "string"
        },
        "businessCode": {
          "type": "string"
        },
        "shoePart": {
          "type": "string"
        },
        "featureName": {
          "type": "string"
        },
        "featureDescription": {
          "type": "string"
        },
        "logoName": {
          "type": "string"
        },
        "logoHref": {
          "type": "string",
          "format": "uri"
        }
      },
      "examples": [{
        "featureAndBenefitCode": "68465468",
        "businessCode": "BU Young Athlete",
