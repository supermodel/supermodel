{
  "$id": "http://localhost:3000/david/adidas/product/Article",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Article",
  "type": "object",
  "properties": {
    "articleNumber": {
      "type": "string"
    },
    "modelNumber": {
      "type": "string"
    },
    "articleDescription": {
      "type": "string"
    },
    "gender": {
      "$ref": "utils/CodeDescription"
    },
    "brand": {
      "$ref": "utils/CodeDescription"
    },
    "productType": {
      "$ref": "utils/CodeDescription"
    },
    "ageGroup": {
      "$ref": "utils/CodeDescription"
    },
    "colorway": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    },
    "colors": {
      "type": "array",
      "items": {
        "$ref": "Color"
      }
    },
    "seasons": {
      "type": "array",
      "items": {
        "$ref": "http://localhost:3000/david/adidas/product/ArticleSeason"
      }
    },
    "skus": {
      "type": "array",
      "items": {
        "$ref": "SKU"
      }
    },
    "assets": {
      "type": "array",
      "items": {
        "$ref": "Asset"
      }
    },
    "catalog": {
      "$ref": "Catalog"
    },
    "related": {
      "type": "array",
      "items": {
        "$ref": "Article"
      }
    }
  },
  "examples": [
    {
      "articleNumber": "015110",
      "modelNumber": "10034",
      "articleDescription": "COPA MUNDIAL",
      "gender": {
        "code": "M",
        "description": "MALE"
      },
      "brand": {
        "code": "11",
        "description": "ADIDAS"
      },
      "productType": {
        "code": "104",
        "description": "FOOTBALL SHOES (FIRM GROUND)"
      },
      "ageGroup": {
        "code": "A",
        "description": "ADULT"
      },
      "colorway": {
        "name": "FTWR WHITE/BLACK",
        "description": "BLABAS/GRAFIT/BLABAS"
      },
      "colors": [{}],
      "seasons": [{}],
      "skus": [{}],
      "assets": [{}],
      "catalog": {}
    }
  ],
  "definitions": {
    "http://localhost:3000/david/adidas/product/utils/CodeDescription": {
      "$id": "http://localhost:3000/david/adidas/product/utils/CodeDescription",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Code Description Tuple",
      "description": "Helper data structure to hold any code - description pair",
      "type": "object",
      "properties": {
        "code": {
          "type": "string"
        },
        "description": {
          "type": "string"
        }
      }
    },
    "http://localhost:3000/david/adidas/product/Color": {
      "$id": "http://localhost:3000/david/adidas/product/Color",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Color",
      "type": "object",
      "description": "Color descriptions are language-dependent",
      "properties": {
        "color1": {
          "$ref": "utils/CodeDescription"
        },
        "color2": {
          "$ref": "utils/CodeDescription"
        },
        "color3": {
          "$ref": "utils/CodeDescription"
        },
        "color4": {
          "$ref": "utils/CodeDescription"
        }
      },
      "examples": [
        {
          "color1": {
            "code": "A0QM",
            "description": "core black"
          },
          "color2": {
            "code": "A98B",
            "description": "utility black"
          },
          "color3": {
            "code": "A0QM",
            "description": "core black"
          },
          "color4": {
            "code": "01F7",
            "description": "ftwr white"
          }
        }
      ]
    },
    "http://localhost:3000/david/adidas/product/ArticleSeason": {
      "$id": "http://localhost:3000/david/adidas/product/ArticleSeason",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Article Season",
      "description": "Seasonal information about article",
      "type": "object",
      "properties": {
        "seasonName": {
          "type": "string"
        },
        "carryOver": {
          "type": "boolean"
        },
        "retailIntroDate": {
          "type": "string",
          "format": "date-time"
        },
        "retailExitDate": {
          "type": "string",
          "format": "date-time"
        }
      },
      "examples": [
        {
          "seasonName": "FW17",
          "carryOver": false,
          "retailIntroDate": "2017-06-01",
          "retailExitDate": "2017-11-30"
        }
      ]
    },
    "http://localhost:3000/david/adidas/product/SKU": {
      "$id": "http://localhost:3000/david/adidas/product/SKU",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "SKU",
      "type": "object",
      "properties": {
        "EAN": {
          "type": "string"
        },
        "UPC": {
          "type": "string"
        },
        "srcSizeCD3": {
          "type": "string"
        },
        "srcSizeScale": {
          "type": "string"
        },
        "srcSizeDescription": {
          "type": "string"
        },
        "conversionIDs": {
          "type": "array",
          "items": [
            {
              "type": "object",
              "properties": {
                "localSize": {
                  "type": "string"
                },
                "saleSizeConvID": {
                  "type": "string"
                },
                "saleSizeScale": {
                  "type": "string"
                },
                "saleSizeAbbr": {
                  "type": "string"
                },
                "saleSizeDesc": {
                  "type": "string"
                }
              }
            }
          ]
        }
      },
      "examples": [
        {
          "EAN": "4058026055128",
          "UPC": "885591427616",
          "srcSizeCD3": "380",
          "srcSizeScale": "T4",
          "conversionIDs": [
            {
              "saleSizeConvID": "D",
              "saleSizeScale": "8C",
              "saleSizeAbbr": "27"
            }
          ]
        }
      ]
    },
    "http://localhost:3000/david/adidas/product/Asset": {
      "$id": "http://localhost:3000/david/adidas/product/Asset",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Asset",
      "description": "Digital Asset",
      "type": "object",
      "properties": {
        "assetType": {
          "type": "string",
          "enum": ["image", "video", "other"]
        },
        "viewGroup": {
          "type": "string"
        },
        "view": {
          "type": "string"
        },
        "assetCategory": {
          "type": "string"
        },
        "usageTerms": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "assetHref": {
          "type": "string",
          "format": "uri"
        },
        "image": {
          "type": "object",
          "properties": {
            "style": {
              "type": "string"
            },
            "width": {
              "type": "integer"
            },
            "height": {
              "type": "integer"
            },
            "bgColor": {
              "type": "string"
            },
            "representation": {
              "type": "string"
            }
          }
        },
        "video": {
          "type": "object",
          "properties": {
            "fileSize": {
              "type": "integer"
            }
          }
        },
        "model": {
          "type": "object",
          "properties": {
            "waist": {
              "type": "string"
            },
            "chest": {
              "type": "string"
            },
            "height": {
              "type": "string"
            },
            "productSize": {
              "type": "string"
            }
          }
        }
      },
      "required": [
        "assetType",
        "viewGroup",
        "view",
        "assetCategory",
        "usageTerms",
        "assetHref"
      ],
      "examples": [
        {
          "assetType": "image",
          "viewGroup": "3d",
          "assetCategory": "Photography",
          "usageTerms": "B2B Cataloguge",
          "assetHref": "http://domain.com/image.jpeg",
          "image": {
            "style": "On Model",
            "width": 2000,
            "height": 2000,
            "bgColor": "transparent"
          },
          "model": {
            "waist": 58,
            "chest": 97,
            "height": 197,
            "productSize": 540
          }
        }
      ]
    },
    "http://localhost:3000/david/adidas/product/Catalog": {
      "$id": "http://localhost:3000/david/adidas/product/Catalog",
      "$schema": "http://json-schema.org/draft-07/schema#",
      "title": "Catalog",
      "type": "object",
      "properties": {
        "catalogueCopy": {
          "$ref": "#/definitions/CatalogCopy"
        },
        "featureAndBenefits": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/FeatureAndBenefit"
          }
        }
      },
      "examples": [
        {
          "catalogueCopy": {
            "a": 1,
            "c": "hello"
          },
          "featureAndBenefits": [
            {
              "b": 2
            }
          ]
        },
        {
          "catalogueCopy": {
            "d": 5,
            "c": "world"
          },
          "featureAndBenefits": [
            {
              "x": 1
            },
            {
              "z": "cus bus"
            }
          ]
        }
      ]
    },
