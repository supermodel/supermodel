{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "array-type": false,
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "quotemark": false,
    "arrow-parens": false,
    "max-line-length": [true, { "limit": 120, "ignore-pattern": "//" }],
    "ordered-imports": false,
    "member-access": false
  },
  "linterOptions": {
    "exclude": [
        "src/**/*.js"
    ]
  }
}
